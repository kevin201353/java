package msgcomm;
public class Display {
	private String host;  
	private String port;
	private String vmId;
	public void setHost(String host){  
	    this.host = host;  
	}  
	public void setPort(String port){  
	    this.port = port;  
	}
	public void setVmId(String vmId){  
	    this.vmId = vmId;  
	}  
	public String getHost(){  
	    return this.host;  
	}  
	public String getPort(){  
	    return this.port;  
	}
	public String getVmId(){  
	    return this.vmId;  
	}  
}