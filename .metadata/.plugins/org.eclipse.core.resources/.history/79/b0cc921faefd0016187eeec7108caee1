package msgcomm;

public class display {
//	private String host = "192.168.0.220";
//	private String port = "5901";
//	private String vmId = "123456";
	private String name;
	      private int age;
	      private String sex;
	      public String getName() {
	          return name;
	      }
	      public void setName(String name) {
	         this.name = name;
	     }
	     public int getAge() {
	         return age;
	     }
	     public void setAge(int age) {
	         this.age = age;
	     }
	     public String getSex() {
	         return sex;
	     }
	     public void setSex(String sex) {
	         this.sex = sex;
	     }
	     public display() {
	         super();
	         // TODO Auto-generated constructor stub
	     }
	     public display(String name, int age, String sex) {
	         super();
	         this.name = name;
	         this.age = age;
	         this.sex = sex;
	     }
	     @Override
	     public String toString() {
	         return "Person [name=" + name + ", age=" + age + ", sex=" + sex + "]";
	}
}
